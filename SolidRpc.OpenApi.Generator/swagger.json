{
  "swagger": "2.0",
  "info": {
    "title": "SolidRpc.Swagger.Generator",
    "version": "1.0.0"
  },
  "host": "localhost",
  "basePath": "/SolidRpc/Swagger/Generator",
  "paths": {
    "/Services/ISwaggerGenerator/CreateSwaggerCode": {
      "post": {
        "tags": [
          "ISwaggerGenerator"
        ],
        "description": "Creates a swagger specification from supplied project.",
        "operationId": "CreateSwaggerCode",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": false,
            "schema": {
              "properties": {
                "settings": {
                  "$ref": "#/definitions/SettingsCodeGen"
                },
                "swaggerFile": {
                  "$ref": "#/definitions/FileData"
                }
              },
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Represents a project",
            "schema": {
              "$ref": "#/definitions/Project"
            }
          }
        }
      }
    },
    "/Services/ISwaggerGenerator/CreateSwaggerSpec": {
      "post": {
        "tags": [
          "ISwaggerGenerator"
        ],
        "description": "Creates a swagger specification from supplied project.",
        "operationId": "CreateSwaggerSpec",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/octet-stream"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": false,
            "schema": {
              "properties": {
                "settings": {
                  "$ref": "#/definitions/SettingsSpecGen"
                },
                "project": {
                  "$ref": "#/definitions/Project"
                }
              },
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The response type",
            "schema": {
              "type": "file"
            }
          }
        }
      }
    },
    "/Services/ISwaggerGenerator/ParseProject": {
      "post": {
        "tags": [
          "ISwaggerGenerator"
        ],
        "description": "Parses the supplied project zip into a project representation.",
        "operationId": "ParseProject",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": false,
            "schema": {
              "properties": {
                "projectZip": {
                  "$ref": "#/definitions/FileData"
                }
              },
              "type": "object"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Represents a project",
            "schema": {
              "$ref": "#/definitions/Project"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "SettingsCodeGen": {
      "type": "object",
      "description": "Settings for generating code from a swagger file."
    },
    "FileData": {
      "properties": {
        "FileStream": {
          "type": "string",
          "format": "binary"
        },
        "ContentType": {
          "type": "string"
        },
        "Filename": {
          "type": "string"
        }
      },
      "type": "object",
      "description": "Represents a file"
    },
    "ProjectFile": {
      "properties": {
        "Directory": {
          "type": "string"
        },
        "FileData": {
          "$ref": "#/definitions/FileData"
        }
      },
      "type": "object",
      "description": "Represents a project file"
    },
    "Project": {
      "properties": {
        "ProjectFiles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ProjectFile"
          }
        }
      },
      "type": "object",
      "description": "Represents a project"
    },
    "SettingsSpecGen": {
      "type": "object",
      "description": "Settings for generating a swagger file from code."
    }
  },
  "tags": [
    {
      "name": "ISwaggerGenerator",
      "description": "The project parser is responsible for parsing project files into representations\r\nthat can be analyzed by the swagger code generators."
    }
  ]
}